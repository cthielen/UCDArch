<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class
    name="UCDArch.RegressionTests.SampleMappings.Unit, UCDArch.RegressionTests"
    table="Unit" >
    <id name="Id" column="UnitID" unsaved-value="0">
      <generator class="identity" />
    </id>

    <property name="FullName" column="FullName" />
    <property name="ShortName" column="ShortName" />
    <property name="PPSCode" column="PPS_Code" />
    <property name="FISCode" column="FIS_Code" />
<!-- TODO: Enable this?
    <many-to-one name="School" column="SchoolCode" class="UCDArch.RegressionTests.SampleMappings.School, UCDArch.RegressionTests"></many-to-one>
-->
  </class>
</hibernate-mapping>
